{
  "name": "scrapbook",
  "author": "Malte Wassmann",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=v14.16",
    "npm": ">=7.7"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "buildEnvTypes": "node scripts/buildEnvTypes.js",
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config electron-builder.config.js --dir",
    "dev": "concurrently --kill-others \"npm run watch\"",
    "watch": "node scripts/watch.mjs",
    "postinstall": "electron-builder install-app-deps"
  },
  "browserslist": [
    "Chrome 96"
  ],
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.32",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^17.0.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.5",
    "rollup": "^2.61.1",
    "svelte": "^3.44.3",
    "svelte-preprocess": "^4.10.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.4"
  },
  "dependencies": {
    "@roxi/routify": "^3.0.0-next.82",
    "electron-is-dev": "^2.0.0"
  },
  "peerDependencies": {
    "rollup": "^2.51.1"
  }
}
