{
  "name": "scrapbook",
  "author": "Malte Wassmann",
  "version": "0.9.2",
  "description": "Library / Viewer for collections of images",
  "private": true,
  "engines": {
    "node": ">=v14.16",
    "npm": ">=7.7"
  },
  "main": "build/main/index.cjs",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.mjs",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder --config electron-builder.config.cjs",
    "dev": "concurrently --kill-others \"npm run watch\"",
    "watch": "node scripts/watch.mjs",
    "postinstall": "electron-builder install-app-deps"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 9999
  },
  "devDependencies": {
    "@iconify/icons-mdi": "^1.2.21",
    "@iconify/svelte": "^2.2.1",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "@types/better-sqlite3": "^7.4.2",
    "@types/heap": "^0.2.31",
    "@types/mime": "^2.0.3",
    "@types/node": "^18.0.0",
    "@types/sharp": "^0.30.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "eslint": "^8.18.0",
    "postcss": "^8.4.14",
    "sass": "^1.53.0",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "vite": "^2.9.12"
  },
  "dependencies": {
    "@floating-ui/dom": "^0.5.4",
    "@fontsource/roboto": "^4.5.7",
    "@material/button": "^14.0.0",
    "@material/checkbox": "^14.0.0",
    "@material/chips": "^14.0.0",
    "@material/dom": "^14.0.0",
    "@material/form-field": "^14.0.0",
    "@material/icon-button": "^14.0.0",
    "@material/list": "^14.0.0",
    "@material/menu": "^14.0.0",
    "@material/radio": "^14.0.0",
    "@material/ripple": "^14.0.0",
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@popmotion/popcorn": "^0.4.4",
    "@roxi/routify": "^3.0.0-next.93",
    "async-mutex": "^0.3.2",
    "better-sqlite3": "^7.5.3",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.2",
    "fuse.js": "^6.6.2",
    "heap": "^0.2.7",
    "immer": "^9.0.15",
    "iso-639-3": "^3.0.1",
    "mime": "^3.0.0",
    "nanoid": "^3.3.4",
    "sharp": "^0.30.7",
    "svelte-forms-lib": "^2.0.0",
    "svelte-motion": "^0.11.2",
    "svelte-portal": "^2.2.0",
    "yup": "^0.32.9"
  }
}
